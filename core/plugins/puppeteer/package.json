{
  "name": "@scullyio/scully-plugin-puppeteer",
  "version": "3.0.0-beta.1",
  "description": "Scully Puppeteer render plugin",
  "main": "./src/index.js",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.4.1",
    "puppeteer": "^19.5.0"
  },
  "type": "module",
  "scripts": {
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc -p tsconfig.json",
      "files": [
        "src/**/*.ts"
      ],
      "output": [
        "lib"
      ],
      "dependencies": [
        "../../../core/cli:build"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@scullyio/scully": "*"
  },
  "repository": {
    "type": "GIT",
    "url": "https://github.com/scullyio/scully/tree/main/libs/plugins/scully-plugin-puppeteer"
  },
  "keywords": [
    "angular",
    "scully",
    "seo",
    "scully-plugin",
    "puppeteer",
    "plugin",
    "render"
  ]
}
